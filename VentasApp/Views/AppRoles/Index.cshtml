@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityRole>

@{
    ViewData["Title"] = "Application Roles";

<H3>Roles</H3>

<p>
    <a asp-action="Create">Crear Rol</a>

</p>

<table class="table">
    <thead>
        <tr>
            <th>
                <p>Nombre</p>
            </th>           
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
            </tr>
        }
    </tbody>
</table>

}