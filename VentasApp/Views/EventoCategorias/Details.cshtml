@model VentasApp.Models.EventoCategoria

@{
    ViewData["Title"] = "Details";
}

<h3>Detalles</h3>

<div>
    <h4>Categoria de Evento</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CategoriaEntrada)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CategoriaEntrada.CategoriaEntradaId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Evento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Evento.EventoId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.EventoCategoriaId">Edit</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
